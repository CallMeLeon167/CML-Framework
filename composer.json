{
  "name": "callmeleon167/cml-framework",
  "description": "A PHP Framework that saves you time and scales with your development process. Provides everything you need to build a Web app. All in one place, designed to work together perfectly.",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=7.4.0",
    "ext-mysqli": "*"
  },
  "authors": [
    {
      "name": "Leon Schmidt",
      "email": "kontakt@callmeleon.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "CML\\Classes\\": "app/classes/",
      "CML\\Controllers\\": "controllers/",
      "CML\\Classes\\Functions\\": "app/classes/Functions/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
